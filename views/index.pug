extends layout

block content
  h1= title
  p Welcome to #{title}
  form(action="/api/v1/videos" method="post" enctype="multipart/form-data")
    input(type="file" name="video")
    input(type="submit" value="UPLOAD")

  p
    video(width="320" height="240" controls)
      source(src="https://s3-us-west-1.amazonaws.com/blue-boomerang/videos/1494651162461/oceans.mp4" type="video/mp4")

  p
    video(width="320" height="240" controls)
      source(src="https://s3-us-west-1.amazonaws.com/blue-boomerang/videos/1494651414018/mov_bbb.mp4" type="video/mp4")
